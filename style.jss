body {
    font-family: Arial, sans-serif;
    padding: 20px;
}

.student {
    padding: 8px;
    border-bottom: 1px solid #ddd;
}

.present {
    color: green;
    font-weight: bold;
}

.absent {
    color: red;
    font-weight: bold;
}

button {
    padding: 10px;
    margin: 10px 0;
    cursor: pointer;
}
